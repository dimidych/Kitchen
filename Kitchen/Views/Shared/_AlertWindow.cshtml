@model string

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="window.location.reload();">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Предупреждение</h4>
</div>

<div class="modal-body">
    <div class="alert alert-danger" role="alert">
        @Model
    </div>
</div>

<div class="modal-footer">
    <button class="btn" data-dismiss="modal" onclick="window.location.reload();">Закрыть</button>
</div>
